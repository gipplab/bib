  booktitle = {Proceedings of the 10th {{NTCIR Conference}} on {{Evaluation}} of {{Information Access Technologies}}, {{NTCIR}}-10, {{National Center}} of {{Sciences}}, {{Tokyo}}, {{Japan}}, {{June}} 18-21, 2013},
